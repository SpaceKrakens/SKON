~Version: 1.0~

def JobInventory:
{
    ObjectType: String,
    Amount: Integer,
},

def BuildingJob:
{
    JobTime: Decimal,
    optional Inventories: [ #JobInventory ],
},

def Tile:
{
    TileType: String,
    Name: String,
    Description: String,
    BaseMovementCost: Decimal,
    LinksToNeighbours: Boolean,

    optional CanPlaceHere: [ String ],

    BuildingJob: #BuildingJob,

    LocalizationCode: String,
    UnlocalizedDescription: String,
},

Tiles: [ #Tile ],